		"csrf-csrf": "^3.0.6",
		"express-session": "^1.18.0",
		"handlebars": "^4.7.8",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"nest-commander": "^3.13.0",
		"nestjs-typeorm-commands": "^0.1.0",
