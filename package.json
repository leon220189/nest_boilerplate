		"@types/body-parser": "^1.19.5",
		"@types/cookie": "^0.6.0",
		"@types/cookie-parser": "^1.4.7",
		"@types/cookie-signature": "^1.1.2",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.18.0",
		"@types/jest": "29.5.12",
		"@types/lodash": "^4.17.5",
		"@types/node": "^20.14.2",
		"@types/nodemailer": "^6.4.15",
